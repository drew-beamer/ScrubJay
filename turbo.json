{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**/*"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "check-types": {
            "dependsOn": ["^check-types"]
        },
        "lint": {
            "dependsOn": ["^lint"]
        }
    }
}
